{
  "name": "chefsire-clean",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "tsx server/index.ts",
    "dev:web": "vite --host",
    "build": "vite build && esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js"
  }
}
